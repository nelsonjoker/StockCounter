    </providers>
  </entityFramework>
  <userSettings>
    <StockCounter.Properties.Settings>
      <setting name="PrinterName" serializeAs="String">
        <value />
      </setting>
      <setting name="PageWidth" serializeAs="String">
        <value>50</value>
      </setting>
      <setting name="PageHeight" serializeAs="String">
        <value>25</value>
      </setting>
      <setting name="PrinterResolution" serializeAs="String">
        <value>-1</value>
      </setting>
      <setting name="Barcode" serializeAs="String">
        <value>Code39</value>
      </setting>
      <setting name="ScannerPort" serializeAs="String">
        <value />
